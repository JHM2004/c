#include <iostream>
using namespace std;
int main()
{
    int ch=0;
    while((ch=getchar())!=EOF)
        putchar(ch);
        return 0;
}


//最简形式：
#include <iostream>

using namespace std;

int main()
{
    int ch=0;
        ch=getchar();
        putchar(ch);
        return 0;
}
